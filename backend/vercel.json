{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/auth/(.*)",
      "dest": "/api/auth.js"
    },
    {
      "src": "/api/expenses/(.*)",
      "dest": "/api/expenses.js"
    },
    {
      "src": "/api/insights",
      "dest": "/api/insights.js"
    }
  ]
}